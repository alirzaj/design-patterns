{
  "name": "alirzaj/design-patterns",
  "type": "project",
  "description": "design pattern examples",
  "keywords": ["design pattern", "example"],
  "license": "MIT",
  "require": {
    "php": "^8.1"
  },
  "require-dev": {},
  "autoload": {
    "psr-4": {
      "factory\\": "factory/",
      "adapter\\": "adapter/",
      "singleton\\": "singleton/",
      "builder\\": "builder/",
      "decorator\\": "decorator/",
      "command\\": "command/"
    }
  },
  "autoload-dev": {"psr-4": {}},
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true,
    "allow-plugins": {}
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}